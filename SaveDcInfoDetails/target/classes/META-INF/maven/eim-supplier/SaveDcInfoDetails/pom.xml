<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>eim-supplier</groupId>
    <artifactId>soa-parent</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <relativePath>../soa-parent</relativePath>
  </parent>
  <artifactId>SaveDcInfoDetails</artifactId>
  <description>Save dc info details</description>
  <name>SaveDcInfoDetails</name>
  	<properties>
		<project_version>1.0.0-SNAPSHOT</project_version>
	</properties>
	<dependencies>
		<dependency>
			<groupId>eim-supplier</groupId>
			<artifactId>eim-services-common</artifactId>
			<version>${project_version}</version>
		</dependency>
	</dependencies>
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.jvnet.jaxb2.maven2</groupId>
					<artifactId>maven-jaxb2-plugin</artifactId>
					<version>0.8.0</version>
					<dependencies>
						<dependency>
							<groupId>com.sun.xml.bind</groupId>
							<artifactId>jaxb-xjc</artifactId>
							<version>2.1.13</version>
						</dependency>
					</dependencies>
					<executions>
						<execution>
							<id>gsm-generate</id>
							<goals>
								<goal>generate</goal>
							</goals>
						</execution>
					</executions>
					<configuration>
						<schemaDirectory>src/main/resources/xsd</schemaDirectory>
						<schemaIncludes>
							<include>airSearch.xsd</include>
						</schemaIncludes>
						<generateDirectory>src/main/java</generateDirectory>
						<bindingDirectory>src/main/resources/binding</bindingDirectory>
						<bindingIncludes>
							<include>saveDcInfo-jaxbBinding_V1.xjb</include>
						</bindingIncludes>
						<strict>false</strict>
						<extension>true</extension>
						<plugins>
							<plugin>
								<groupId>org.jvnet.jaxb2_commons</groupId>
								<artifactId>jaxb2-basics</artifactId>
								<version>0.6.2</version>
							</plugin>
						</plugins>
						<args>
							<arg>-Xsetters</arg>
							<arg>-XtoString</arg>
							<arg>-Xequals</arg>
						</args>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>org.jvnet.jaxb2.maven2</groupId>
										<artifactId>maven-jaxb2-plugin</artifactId>
										<versionRange>[0.8.0,)</versionRange>
										<goals>
											<goal>generate</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
</project>